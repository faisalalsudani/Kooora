<%= form_for [@country, Team.new] do |f| %>

  <div class="form-group">
    <label>Team name</label>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <label>Team logo</label>
    <%= f.text_field :logo, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit "Save", class: "btn btn-primary" %>
  </div>
<% end %>
